{
  "version": 2,
  "name": "vats",
  "alias": "vats",
  "builds": [
    {
      "src": "deployables/cra/package.json",
      "use": "@now/static-build",
      "config": { "distDir": "build" }
    },
    { "src": "deployables/apollo/src/index.ts", "use": "@now/node-server@canary" }
  ],
  "routes": [
    {
      "src": "^/cra/static/(.*)",
      "headers": { "cache-control": "s-maxage=31536000,immutable" },
      "dest": "deployables/cra/static/$1"
    },
    { "src": "^/cra/favicon.ico", "dest": "deployables/cra/favicon.ico" },
    {
      "src": "^/cra/asset-manifest.json",
      "dest": "deployables/cra/asset-manifest.json"
    },
    { "src": "^/cra/manifest.json", "dest": "deployables/cra/manifest.json" },
    {
      "src": "^/cra/precache-manifest.(.*)",
      "dest": "deployables/cra/precache-manifest.$1"
    },
    {
      "src": "^/cra/service-worker.js",
      "headers": { "cache-control": "s-maxage=0" },
      "dest": "deployables/cra/service-worker.js"
    },
    {
      "src": "^/cra(.*)",
      "headers": { "cache-control": "s-maxage=0" },
      "dest": "deployables/cra/index.html"
    },
    { "src": "^/apollo(.*)", "dest": "deployables/apollo/src/index.ts" }
  ]
}
