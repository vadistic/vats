{
  "name": "@vats/client",
  "version": "0.1.0",
  "description": "vats client app",
  "author": "Jakub Wadas <vadistic@gmail.com>",
  "license": "No license",
  "private": true,
  "scripts": {
    "start": "vscripts craco start",
    "build": "vscripts craco build",
    "test": "vscripts craco test",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "env": "node ./scripts/env-cli",
    "gen:server": "graphql get-schema -p server",
    "gen:client:sdl": "yarn ts-node --project './scripts/tsconfig.json' scripts/client-sdl",
    "gen:client:resolvers": "gql-gen --config codegen.yml",
    "gen:client:frag": "graphql generate-fragments",
    "gen:client": "yarn gen:client:sdl && yarn gen:client:resolvers && yarn gen:client:frag",
    "gen:queries": "yarn apollo client:codegen src/generated/queries.ts --outputFlat --target typescript",
    "gen:i18n": "ts-node --project './scripts/tsconfig.json' scripts/i18n-json",
    "gen": "yarn gen:server && yarn gen:client && yarn gen:queries && yarn gen:i18n",
    "cosmos": "cosmos"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "dependencies": {
    "@emotion/core": "^10.0.7",
    "@martin_hotell/rex-tils": "^2.1.1",
    "@reach/router": "^1.2.1",
    "@uifabric/icons": "^6.4.0",
    "@uifabric/styling": "6.43.0",
    "@vats/i18n": "^0.1.0",
    "@vats/utils": "^0.1.0",
    "apollo-cache-inmemory": "^1.5.0-rc.1",
    "apollo-client": "^2.5.0-rc.1",
    "apollo-link": "^1.2.8",
    "apollo-link-error": "^1.1.7",
    "apollo-link-http": "^1.5.11",
    "clone-deep": "^4.0.1",
    "dayjs": "^1.8.7",
    "deep-diff": "^1.0.2",
    "formik": "vadistic/formik#formik-v2.0.0-gamma.1-gitpkg",
    "fuzzy-search": "^3.0.1",
    "graphql": "^14.1.1",
    "graphql-tag": "^2.10.1",
    "immer": "^2.0.0",
    "office-ui-fabric-react": "6.148.0",
    "react": "^16.8.3",
    "react-apollo": "^2.5.0-rc.1",
    "react-apollo-hooks": "^0.4.1",
    "react-dom": "^16.8.3",
    "react-transition-group": "^2.5.3",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "@types/deep-diff": "^1.0.0",
    "@types/fuzzy-search": "^2.1.0",
    "@types/jest": "^24.0.6",
    "@types/node": "^11.9.5",
    "@types/reach__router": "^1.2.3",
    "@types/react": "^16.8.4",
    "@types/react-dom": "^16.8.2",
    "@types/react-transition-group": "^2.0.16",
    "@types/uuid": "^3.4.4",
    "apollo": "^2.5.1",
    "graphql-cli": "^3.0.11",
    "graphql-cli-generate-fragments": "^1.4.1",
    "graphql-code-generator": "0.18.0",
    "graphql-codegen-introspection": "0.18.0",
    "graphql-codegen-typescript-common": "0.18.0",
    "graphql-codegen-typescript-resolvers": "0.18.0",
    "graphql-codegen-typescript-server": "0.18.0",
    "jest-dom": "^3.1.2",
    "react-testing-library": "^6.0.0",
    "redux": "^4.0.1",
    "reinspect": "vadistic/reinspect#reinspect-v0.0.3-test-gitpkg",
    "typescript": "^3.3.3333"
  },
  "resolutions": {
    "graphql": "14.1.1"
  }
}
