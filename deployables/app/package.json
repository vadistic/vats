    "start": "vscripts craco start",
    "build": "vscripts craco build",
    "now-build": "vscripts craco build",
    "test": "vscripts test",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "serve": "vscripts serve -s",
    "storybook": "vscripts storybook",
    "env": "node ./scripts/env-cli",
