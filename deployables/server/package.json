{
  "name": "@vats/server",
  "version": "0.1.0",
  "description": "vats graphql server",
  "author": "Jakub Wadas <vadistic@gmail.com>",
  "license": "No license",
  "private": true,
  "main": "build/index.js",
  "source": "src/index.ts",
  "scripts": {
    "prepare": "yarn gen:sdl",
    "build": "vscripts backpack build",
    "dev": "vscripts backpack dev",
    "test": "echo todo",
    "start": "node build/main.js",
    "typecheck": "tsc -p . --noEmit",
    "apollo:push": "apollo service:push",
    "copyfiles": "vscripts copyfiles src/**/*.{json,graphql}",
    "copy:prisma": "cp ../prisma/src/generated/prisma.graphql ./src/generated",
    "gen:sdl": "vscripts runtime scripts/server-sdl",
    "gen:ts": "gql-gen --config codegen.yml",
    "gen": "yarn gen:sdl && yarn gen:ts"
  },
  "dependencies": {
    "@vats/prisma": "^0.1.0",
    "@vats/utils": "^0.1.0",
    "apollo-server": "^2.4.0",
    "aws-sdk": "^2.401.0",
    "dayjs": "^1.8.5",
    "gql-import": "vadistic/gql-import#gql-import-v0.1.2-gitpkg",
    "graphql": "^14.1.1",
    "graphql-import": "^0.7.1",
    "graphql-middleware": "^3.0.2",
    "graphql-shield": "^5.1.3",
    "graphql-tools": "^4.0.4",
    "graphql-upload": "^8.0.0",
    "jsonwebtoken": "^8.3.0",
    "jwks-rsa": "^1.4.0",
    "prisma-binding": "^2.3.2"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/graphql": "^14.0.6",
    "@types/jsonwebtoken": "^8.3.0",
    "@vats/start": "^0.1.0",
    "apollo": "^2.4.4",
    "graphql-cli": "^3.0.9",
    "graphql-code-generator": "0.18.0",
    "graphql-codegen-introspection": "0.18.0",
    "graphql-codegen-typescript-common": "0.18.0",
    "graphql-codegen-typescript-resolvers": "0.18.0",
    "graphql-codegen-typescript-server": "0.18.0",
    "typescript": "^3.3.3"
  },
  "resolutions": {
    "graphql": "14.1.1"
  }
}
