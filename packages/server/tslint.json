{
  "extends": ["tslint:latest", "tslint-config-prettier"],
  "rules": {
    "no-shadowed-variable": true,
    "no-console": false,
    "object-literal-sort-keys": false,
    "no-empty-interface": false,
    "variable-name": [
      true,
      "ban-keywords",
      "check-format",
      "allow-pascal-case",
      "allow-leading-underscore"
    ]
  },
  "linterOptions": {
    "exclude": ["src/generated/*"]
  }
}
