  "license": "No license",
  "private": true,
  "source": "src/index.ts",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "sideEffects": [
    "**/i18n.js"
  ],
  "scripts": {
    "clean": "rm -rf dist",
    "transpile": "vscripts transpile",
