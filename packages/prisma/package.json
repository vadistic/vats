{
  "name": "prisma",
  "version": "0.1.0",
  "description": "Prisma service deployment toolkit",
  "author": "Jakub Wadas <vadistic@gmail.com>",
  "license": "No license",
  "private": true,
  "scripts": {
    "down": "docker-compose down",
    "up": "docker-compose up -d --force-recreate",
    "env:cmd": "env-cmd ../../.env",
    "env:pull": "shx cp ../../.env .env",
    "env:push": "shx cp .env ../../.env"
  },
  "devDependencies": {
    "env-cmd": "^8.0.2",
    "shx": "^0.3.2"
  }
}
