{
  "name": "app",
  "version": "0.0.1",
  "author": "Jakub Wadas <vadistic@gmail.com>",
  "license": "No license",
  "private": true,
  "scripts": {
    "env:cmd": "env-cmd ../../.env",
    "env:pull": "shx cp ../../.env .env",
    "env:push": "shx cp .env ../../.env",
    "env:graphql": "yarn env:cmd node scripts/env-gql",
    "dev": "webpack-dev-server",
    "now:build": "parcel build ./src/index.html --no-source-maps",
    "now:deploy": "now ./dist -A ../now.json && now alias",
    "now:": "yarn now:build && yarn now:deploy",
    "apollo:clear": "shx rm -rf src/**/generated/*.ts && shx rm src/generated/schema.json",
    "apollo:schema": " shx cp scripts/apollo.config.js apollo.config.js && yarn env:cmd apollo schema:download src/generated/schema.json && shx rm apollo.config.js",
    "apollo:codegen": "apollo codegen:generate generated  --queries src/**/*.ts --schema src/generated/schema.json --clientSchema src/state/schema.graphql --globalTypesFile src/generated/globalTypes.ts --target typescript --addTypename && shx rm -rf generated",
    "apollo:": "yarn apollo:clear && yarn apollo:schema && yarn apollo:codegen",
    "typecheck": "tsc --noEmit",
    "lint": "tslint --project .",
    "format": "prettier --write src/**/*.{ts,tsx}"
  },
  "dependencies": {
    "@aws-amplify/auth": "^1.1.2",
    "@babel/runtime": "^7.0.0",
    "@reach/router": "^1.1.1",
    "@uifabric/experiments": "^6.30.1",
    "apollo-cache-inmemory": "^1.2.10",
    "apollo-client": "^2.4.2",
    "apollo-link": "^1.2.2",
    "apollo-link-context": "^1.0.8",
    "apollo-link-error": "^1.1.0",
    "apollo-link-http": "^1.5.4",
    "apollo-link-state": "^0.4.1",
    "apollo-upload-client": "^8.1.0",
    "dayjs": "^1.7.5",
    "emotion": "^9.2.8",
    "emotion-theming": "^9.2.6",
    "formik": "^1.3.0",
    "graphql": "^14.0.2",
    "graphql-tag": "^2.9.2",
    "office-ui-fabric-react": "^6.69.0",
    "query-string": "^6.1.0",
    "ramda": "^0.25.0",
    "react": "^16.5.0",
    "react-apollo": "^2.1.11",
    "react-dom": "^16.5.0",
    "react-emotion": "^9.2.8",
    "react-helmet": "^5.2.0",
    "react-powerplug": "^1.0.0-rc.1",
    "styled-system": "^3.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.0.1",
    "@babel/plugin-proposal-class-properties": "^7.0.0",
    "@babel/plugin-proposal-decorators": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.1.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.0.0",
    "@types/apollo-upload-client": "^8.1.0",
    "@types/graphql": "^14.0.0",
    "@types/query-string": "^6.1.0",
    "@types/ramda": "^0.25.38",
    "@types/reach__router": "^1.0.1",
    "@types/react": "^16.4.14",
    "@types/react-dom": "^16.0.7",
    "@types/react-router-dom": "^4.3.0",
    "@types/styled-system": "^3.0.4",
    "apollo": "^1.9.2",
    "babel-loader": "^8.0.2",
    "dotenv-webpack": "^1.5.7",
    "env-cmd": "^8.0.2",
    "fork-ts-checker-webpack-plugin": "^0.4.9",
    "graphql-tools": "^3.1.1",
    "html-webpack-plugin": "^3.2.0",
    "merge-graphql-schemas": "^1.5.4",
    "prettier": "^1.14.2",
    "react-hot-loader": "^4.3.7",
    "react-loadable": "^5.5.0",
    "shx": "^0.3.2",
    "source-map-loader": "^0.2.4",
    "ts-loader": "^5.1.0",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.15.0",
    "tslint-react": "^3.6.0",
    "typescript": "^3.0.3",
    "webpack": "^4.19.0",
    "webpack-bundle-analyzer": "^3.0.2",
    "webpack-cli": "^3.1.0",
    "webpack-dev-server": "^3.1.8"
  },
  "resolutions": {
    "graphql": "^14.0.2"
  }
}
