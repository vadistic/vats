{
  "name": "client",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "craco start",
    "build": "craco build",
    "test": "craco test",
    "format": "prettier --write --no-color -l src/**/*.{ts,tsx}",
    "lint": "tslint -p tsconfig.json",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "validate": "yarn typecheck && yarn lint && yarn format",
    "env": "node scripts/env-config",
    "gen:server": "graphql get-schema -p server",
    "gen:client:sdl": "yarn ts-node --project './scripts/tsconfig.json' scripts/client-sdl",
    "gen:client:resolvers": "gql-gen --config codegen.yml",
    "gen:client:frag": "graphql generate-fragments",
    "gen:client": "yarn gen:client:sdl && yarn gen:client:resolvers && yarn gen:client:frag",
    "gen:queries": "yarn apollo client:codegen src/generated/queries.ts --outputFlat --target typescript",
    "gen": "yarn gen:server && yarn gen:client && yarn gen:queries",
    "serve": "serve build -s",
    "cosmos": "cosmos",
    "cosmos:export": "env-cmd .env.development cosmos-export"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn typecheck && lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "tslint --fix",
      "pretty-quick --staged",
      "git add"
    ]
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "dependencies": {
    "@craco/craco": "^3.4.1",
    "@emotion/core": "^10.0.7",
    "@emotion/styled": "^10.0.7",
    "@reach/router": "^1.2.1",
    "@uifabric/icons": "^6.3.0",
    "@uifabric/styling": "^6.41.0",
    "apollo-cache-inmemory": "^1.5.0-beta.0",
    "apollo-client": "2.5.0-beta.0",
    "apollo-link": "^1.2.8",
    "apollo-link-error": "^1.1.7",
    "apollo-link-http": "^1.5.11",
    "emotion-theming": "^10.0.7",
    "formik": "^2.0.0-gamma.1",
    "graphql": "^14.1.1",
    "graphql-tag": "^2.10.1",
    "immer": "^2.0.0",
    "office-ui-fabric-react": "^6.135.0",
    "react": "^16.8.1",
    "react-apollo": "^2.5.0-beta.1",
    "react-apollo-hooks": "^0.3.1",
    "react-dom": "^16.8.1",
    "react-scripts": "2.1.3",
    "react-transition-group": "^2.5.3",
    "reinspect": "vadistic/reinspect#reinspect-v0.4.3-gitpkg",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "@emotion/babel-preset-css-prop": "^10.0.7",
    "@types/jest": "^24.0.0",
    "@types/node": "^10.12.21",
    "@types/reach__router": "^1.2.3",
    "@types/react": "^16.8.2",
    "@types/react-dom": "^16.8.0",
    "@types/react-transition-group": "^2.0.15",
    "@types/uuid": "^3.4.4",
    "@types/webpack-env": "^1.13.7",
    "apollo": "^2.4.4",
    "dotenv": "^6.2.0",
    "graphql-cli": "^3.0.9",
    "graphql-cli-generate-fragments": "vadistic/graphql-cli-generate-fragments#graphql-tag",
    "graphql-code-generator": "^0.16.1",
    "graphql-codegen-introspection": "^0.16.1",
    "graphql-codegen-typescript-common": "^0.16.1",
    "graphql-codegen-typescript-resolvers": "^0.16.1",
    "graphql-codegen-typescript-server": "^0.16.1",
    "husky": "^1.3.1",
    "jest-dom": "^3.0.2",
    "lint-staged": "^8.1.3",
    "prettier": "^1.16.4",
    "pretty-quick": "^1.10.0",
    "react-cosmos": "^4.7.0",
    "react-testing-library": "^5.5.3",
    "serve": "^10.1.2",
    "ts-node": "^8.0.2",
    "tslint": "^5.12.1",
    "tslint-config-prettier": "^1.18.0",
    "tslint-react": "^3.6.0",
    "typescript": "^3.3.1"
  },
  "resolutions": {
    "apollo-client": "2.5.0-beta.0",
    "react-apollo": "2.5.0-beta.1",
    "graphql": "14.1.1"
  }
}
